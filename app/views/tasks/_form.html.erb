 <%= form_with model: @task do |f| %>
  <div class="my-3">
    <%= f.label :title %>
    <%= f.text_field :title, class:"form-control" %>
  </div>
  <div  class="my-3">
    <%= f.label :details %>
    <%= f.text_field :details, class:"form-control" %>
  </div>
  <div  class="my-3">
    <%= f.label :completed? %>
    <%= f.check_box :completed %>
  </div>

  <%= f.submit "Submit", class:"btn btn-warning my-3"%>
<% end %>
